{
  "name": "quote.garden",
  "version": "1.0.1",
  "description": "A place to save & serve your favorite quotes",
  "engine": {
    "node": "8.2.1",
    "npm": "5.3.0"
  },
  "scripts": {
    "build": "rm -rf docs/* && webpack -p",
    "watch": "webpack-dev-server --profile --progress --colors",
    "prettier": "prettier --no-semi --single-quote --trailing-comma all --write 'src/**/*.+(ts|tsx|js|json)'",
    "tslint": "tslint --fix -p tsconfig.json --exclude 'node_modules/*'",
    "style": "npm run tslint && npm run prettier"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexburner/quote.garden.git"
  },
  "author": "aburner",
  "license": "MIT",
  "dependencies": {
    "@types/react": "16.0.2",
    "@types/react-dom": "15.5.3",
    "@types/react-redux": "5.0.1",
    "@types/redux": "3.6.0",
    "lodash": "4.17.4",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-redux": "5.0.6",
    "redux": "3.7.2"
  },
  "devDependencies": {
    "awesome-typescript-loader": "3.1.3",
    "copy-webpack-plugin": "4.0.1",
    "html-webpack-include-assets-plugin": "0.0.5",
    "html-webpack-plugin": "2.28.0",
    "prettier": "1.5.3",
    "source-map-loader": "0.2.1",
    "tslint": "5.6.0",
    "tslint-config-prettier": "1.3.0",
    "tslint-eslint-rules": "4.1.1",
    "tslint-react": "3.2.0",
    "typescript": "2.4.2",
    "webpack": "2.5.1",
    "webpack-dev-server": "2.4.5"
  }
}
