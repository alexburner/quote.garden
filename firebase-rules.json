{
  "rules": {
        "quotes": {
      ".read": true,
      "$user_id": {
        ".write": "$user_id === auth.uid",
        "$quote_id": {
            ".validate": "
              newData.child('words').val().length > 0 &&
              newData.child('source').val().length > 0
            "
        }
      }
    },
    "profiles": {
      ".read": true,
      "$user_id": {
        ".write": "$user_id === auth.uid",
      }
    },
  }
}